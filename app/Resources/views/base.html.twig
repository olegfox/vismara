<!DOCTYPE html>
<html {% block html %}{% endblock %}>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
    {#<meta name="viewport" content="width=device-width, user-scalable=no">#}
    <title>{% block title %}VISMARA DESIGN{% endblock %}</title>
    <meta name="description" content="{% block description %}{% endblock %}">
    <meta property="og:title" content="{% block og_title %}{% endblock %}"/>
    <meta property="og:description" content="{% block og_description %}{% endblock %}"/>
    <meta property="og:image" content="{% block og_image %}{% endblock %}"/>
    <meta property="og:url" content="{% block og_url %}{% endblock %}" />
    <meta property="og:type" content="article">
    <meta property="og:locale" content="ru_RU">
    <meta property="og:site_name" content="VismaraDesign" />
    <link rel="image_src" href="{% block image_src %}{% endblock %}" />
    {% block stylesheets %}{% endblock %}
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}"/>
</head>
<body>
{% block body %}{% endblock %}
{% block javascripts %}{% endblock %}
</body>
</html>
